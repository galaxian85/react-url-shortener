{
  "name": "turbo-chainsaw",
  "version": "0.1.0",
  "description": "A simple node/react URL shortener",
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.1",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/react-dom": "^17.0.9",
    "@types/sqlite3": "^3.1.7",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.25.1",
    "mini-css-extract-plugin": "^2.2.0",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "build": "webpack --config webpack.server.js && webpack --config webpack.client.js",
    "start": "node build/bundle.js",
    "dev": "npm-run-all --parallel dev:server dev:build:*",
    "dev:server": "nodemon --inspect build/bundle.js",
    "dev:build:server": "webpack --config webpack.server.js --watch",
    "dev:build:client": "webpack --config webpack.client.js --watch"
  }
}
